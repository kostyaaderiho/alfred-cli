{
    "name": "reacttemplate",
    "description": "React template project",
    "scripts": {
        "alfred": "alfred",
        "build": "alfred build",
        "develop": "alfred develop",
        "clean": "npm cache clean --force && rimraf -- dist node_modules",
        "all:test": "npm run test:coverage && npm run storybook:test",
        "test": "jest --config config/jest.config.js",
        "test:watch": "npm run test -- --watchAll",
        "test:update": "npm run test -- -u",
        "test:coverage": "npm run test -- --coverage",
        "test:stryker": "stryker run config/stryker.config.js",
        "lint:scss": "stylelint src/**/*.scss",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "lint:js": "eslint src/**/*.js",
        "lint:js:fix": "npm run lint:js -- --fix",
        "storybook": "start-storybook -p 6006",
        "storybook:build": "build-storybook --quiet",
        "storybook:test": "npm run storybook:build && jest --config .storybook/jest.config.js",
        "storybook:test:update": "npm run storybook:test -- -u"
    },
    "repository": {
        "type": "git",
        "url": "https://git1.corp.globoforce.com/front-end/react-template-project"
    },
    "private": true,
    "author": {
        "name": "Author Name",
        "email": "author@email.com"
    },
    "contributors": [
        "Author Name1 <author@email1.com>",
        "Author Name2 <author@email2.com>"
    ],
    "keywords": [
        "javascript",
        "reactjs",
        "node",
        "projects",
        "sass"
    ],
    "browserslist": [
        "Explorer 11",
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 ChromeAndroid versions",
        "last 2 FirefoxAndroid versions",
        "last 2 Safari versions",
        "last 2 iOS versions",
        "last 2 Edge versions"
    ],
